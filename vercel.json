{
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "Strict-Transport-Security",
                    "value": "max-age=2592000; includeSubDomains; preload"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "SAMEORIGIN"
                },
                {
                    "key": "Content-Security-Policy",
                    "value": "default-src https: ; script-src https: 'unsafe-inline' https://apis.google.com ; style-src https: 'unsafe-inline' ; img-src data: https: 'unsafe-inline' *://*.reserbaja.com:*"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                }
            ]
        }
    ]
}
